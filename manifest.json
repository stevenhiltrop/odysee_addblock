{
  "manifest_version": 3,
  "author": "Steven Hiltrop",
  "name": "Odysee addblocker",
  "description": "This extension will remove adds without login in.",
  "version": "1.0",
  
  
  "icons": { 
    "16": "icon16.png",
    "48": "icon48.png",
   "128": "icon128.png" 
  },

  "content_scripts": [
    {
      "matches": ["https://odysee.com/*"],
      "run_at": "document_idle",
      "js": ["content-script.js"]
    }
  ]
}
